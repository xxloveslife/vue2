       <p>\r\n          <span style=\"font-size:14px\">¥</span>\r\n          {{Number(goods.salePrice).toFixed(2)}}\r\n        </p>\r\n      </div>\r\n    </el-card>\r\n  </el-col>\r\n</el-row>\n</template>\r\n<script>\nimport {mapState} from 'vuex'\nimport {getStore} from '@/utils/storage'\nexport default {\r\n  name: \"\",\r\n\r\n  props:['goods',],\r\n  computed: {\r\n    ...mapState(['login'])\r\n  },\r\n  methods: {\r\n    productDetail(id) {\r\n      // console.log('chakan');\r\n      this.$router.push({\r\n        name:'goodsDetail',\r\n        query:{\r\n          productId:id,\r\n        }\r\n      });\r\n    },\r\n    addCart(id, price, name, img) {\r\n      if (this.login) {\r\n        // 用户已登录\r\n        this.$http.post('api/addCart', {\r\n          userId: getStore(\"id\"),\r\n          productId:id,\r\n          productNum:1\r\n        });\r\n        // \r\n      }else {\r\n        // 如果用户未登录,也将商品存到store的cartList中\r\n        this.ADDCART({\r\n          productId:id,\r\n          salePrice:price,\r\n          productName:name,\r\n          productImageBig:img\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\n</script>\r\n\r\n<style lang=\"scss\" scoped>\n.good-img {\n    display: flex;\n    justify-content: center;\n    a {\n        display: block;\n        img {\n            margin: 50px auto 10px;\n            width: 206px;\n            height: 206px;\n            display: block;\n        }\n    }\n}\n.good-price {\n    margin: 15px 0;\n    height: 30px;\n    text-align: center;\n    line-height: 30px;\n    color: #d44d44;\n    font-family: Arial;\n    font-size: 18px;\n    font-weight: 700;\n    display: flex;\n    justify-content: space-around;\n    padding-bottom: 60px;\n    a {\n        margin-right: 5px;\n    }\n    .ds {\n        display: none;\n    }\n}\n.good-price:hover .ds {\n    display: block;\n}\n.good-title {\n    line-height: 1.2;\n    font-size: 16px;\n    color: #424242;\n    margin: 0 auto;\n    padding: 0 14px;\n    text-align: center;\n    overflow: hidden;\n}\nh3 {\n    text-align: center;\n    line-height: 1.2;\n    font-size: 12px;\n    color: #d0d0d0;\n    padding: 10px;\n}\n.good-item {\n    background: #fff;\n    width: 25%;\n    transition: all 0.5s;\n    height: 410px;\n    &:hover {\n        transform: translateY(-3px);\n        box-shadow: 1px 1px 20px #999;\n        .good-price p {\n            display: none;\n        }\n        .ds {\n            display: flex;\n        }\n    }\n}\n.el-card {\n    border: none;\n}\n</style>\r\n"]}]}